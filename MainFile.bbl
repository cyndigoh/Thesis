    \field{pages}{80}
    \field{title}{HLA alleles modulate EBV viral load in multiple sclerosis.}
    \field{volume}{16}
    \list{location}{1}{%
      {England}%
    }
    \field{journaltitle}{Journal of translational medicine}
    \field{year}{2018}
