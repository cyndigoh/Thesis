  title-abbreviation     = {Nat Microbiol},
}

@Article{Gosiewski2017,
  author                 = {Gosiewski, T. and Ludwig-Galezowska, A. H. and Huminska, K. and Sroka-Oleksiak, A. and Radkowski, P. and Salamon, D. and Wojciechowicz, J. and Kus-Slowinska, M. and Bulanda, M. and Wolkow, P. P.},
  title                  = {Comprehensive detection and identification of bacterial DNA in the blood of patients with sepsis and healthy volunteers using next-generation sequencing method - the observation of DNAemia.},
  journal                = {European journal of clinical microbiology \& infectious diseases : official publication of the European Society of Clinical Microbiology},
  year                   = {2017},
  volume                 = {36},
  pages                  = {329-336},
  month                  = {Feb},
  abstract               = {Blood is considered to be a sterile microenvironment, in which bacteria appear only periodically. Previously used methods allowed only for the detection of either viable bacteria with low sensitivity or selected species of bacteria. The Next-Generation Sequencing method (NGS) enables the identification of all bacteria in the sample with their taxonomic classification. We used NGS for the analysis of blood samples from healthy volunteers (n = 23) and patients with sepsis (n = 62) to check whether any bacterial DNA exists in the blood of healthy people and to identify bacterial taxonomic profile in the blood of septic patients. The presence of bacterial DNA was found both in septic and healthy subjects; however, bacterial diversity was significantly different (P = 0.002) between the studied groups. Among healthy volunteers, a significant predominance of anaerobic bacteria (76.2 %), of which most were bacteria of the order Bifidobacteriales (73.0 %), was observed. In sepsis, the majority of detected taxa belonged to aerobic or microaerophilic microorganisms (75.1 %). The most striking difference was seen in the case of Actinobacteria phyla, the abundance of which was decreased in sepsis (P < 0.001) and Proteobacteria phyla which was decreased in the healthy volunteers (P < 0.001). Our research shows that bacterial DNA can be detected in the blood of healthy people and that its taxonomic composition is different from the one seen in septic patients. Detection of bacterial DNA in the blood of healthy people may suggest that bacteria continuously translocate into the blood, but not always cause sepsis; this observation can be called DNAemia.},
  address                = {Germany},
  article-doi            = {10.1007/s10096-016-2805-7},
  article-pii            = {10.1007/s10096-016-2805-7},
  completed              = {20170215},
  electronic-issn        = {1435-4373},
  electronic-publication = {20161022},
  history                = {2016/10/25 06:00 [entrez]},
  issue                  = {2},
  keywords               = {Aged, Bacteria/classification/genetics, Bacteriological Techniques/*methods, Biodiversity, *Blood Chemical Analysis, DNA, Bacterial/*blood, Female, *Healthy Volunteers, High-Throughput Nucleotide Sequencing/*methods, Humans, Male, Middle Aged, *Sepsis},
  language               = {eng},
  linking-issn           = {0934-9723},
  location-id            = {10.1007/s10096-016-2805-7 [doi]},
  nlm-unique-id          = {8804297},
  owner                  = {NLM},
  publication-status     = {ppublish},
  registry-number        = {0 (DNA, Bacterial)},
  revised                = {20181113},
