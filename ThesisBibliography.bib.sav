@Article{Langley2013,
  author             = {Langley, Raymond J. and Tsalik, Ephraim L. and van Velkinburgh, Jennifer C. and Glickman, Seth W. and Rice, Brandon J. and Wang, Chunping and Chen, Bo and Carin, Lawrence and Suarez, Arturo and Mohney, Robert P. and Freeman, Debra H. and Wang, Mu and You, Jinsam and Wulff, Jacob and Thompson, J. Will and Moseley, M. Arthur and Reisinger, Stephanie and Edmonds, Brian T. and Grinnell, Brian and Nelson, David R. and Dinwiddie, Darrell L. and Miller, Neil A. and Saunders, Carol J. and Soden, Sarah S. and Rogers, Angela J. and Gazourian, Lee and Fredenburgh, Laura E. and Massaro, Anthony F. and Baron, Rebecca M. and Choi, Augustine M. K. and Corey, G. Ralph and Ginsburg, Geoffrey S. and Cairns, Charles B. and Otero, Ronny M. and Fowler, Vance G. Jr and Rivers, Emanuel P. and Woods, Christopher W. and Kingsmore, Stephen F.},
  title              = {An integrated clinico-metabolomic model improves prediction of death in sepsis.},
  journal            = {Science translational medicine},
  year               = {2013},
  volume             = {5},
  pages              = {195ra95},
  month              = {Jul},
  abstract           = {Sepsis is a common cause of death, but outcomes in individual patients are difficult to predict. Elucidating the molecular processes that differ between sepsis patients who survive and those who die may permit more appropriate treatments to be deployed. We examined the clinical features and the plasma metabolome and proteome of patients with and without community-acquired sepsis, upon their arrival at hospital emergency departments and 24 hours later. The metabolomes and proteomes of patients at hospital admittance who would ultimately die differed markedly from those of patients who would survive. The different profiles of proteins and metabolites clustered into the following groups: fatty acid transport and beta-oxidation, gluconeogenesis, and the citric acid cycle. They differed consistently among several sets of patients, and diverged more as death approached. In contrast, the metabolomes and proteomes of surviving patients with mild sepsis did not differ from survivors with severe sepsis or septic shock. An algorithm derived from clinical features together with measurements of five metabolites predicted patient survival. This algorithm may help to guide the treatment of individual patients with sepsis.},
  address            = {United States},
  article-doi        = {10.1126/scitranslmed.3005893},
  article-pii        = {5/195/195ra95},
  completed          = {20140207},
  electronic-issn    = {1946-6242},
  grantno            = {P20 RR016480/RR/NCRR NIH HHS/United States},
