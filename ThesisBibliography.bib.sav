@Article{Kim2011,
  author                 = {Kim, Arthur Y. and Kuntzen, Thomas and Timm, Joerg and Nolan, Brian E. and Baca, Melanie A. and Reyor, Laura L. and Berical, Andrew C. and Feller, Andrea J. and Johnson, Kristin L. and Schulze zur Wiesch, Julian and Robbins, Gregory K. and Chung, Raymond T. and Walker, Bruce D. and Carrington, Mary and Allen, Todd M. and Lauer, Georg M.},
  title                  = {Spontaneous control of HCV is associated with expression of HLA-B 57 and preservation of targeted epitopes.},
  journal                = {Gastroenterology},
  year                   = {2011},
  volume                 = {140},
  pages                  = {686-696.e1},
  month                  = {Feb},
  abstract               = {BACKGROUND & AIMS: HLA class I alleles are linked to spontaneous control of hepatitis C virus (HCV) and human immunodeficiency virus-1, but for HCV the roles of particular alleles and corresponding CD8(+) T-cell responses remain incompletely defined. We aimed to determine the correlations between these alleles and natural outcomes of HCV and determine associated key T-cell responses. METHODS: In a cohort of HCV individuals, we determined HLA class I alleles, HCV outcomes, T-cell responses, and examined sequence data for mutational changes within key epitopes. RESULTS: Carriage of HLA-B 57 was associated with a higher rate of viral clearance (risk ratio = 2.0; 95% confidence interval: 1.2-3.4), while HLA-B 08 was associated with a lower rate (risk ratio = 0.34; 95% confidence interval: 0.1-0.9]. Two HLA-B 57-restricted T-cell epitopes were targeted in spontaneous clearance; subjects with chronic viremia expressing HLA-B 57 harbored HCV strains with a high frequency of mutations in key residues. HLA-B 57-mediated escape was supported by diminished immune recognition of these variants and acute HCV infection revealing viral evolution toward less recognized variants. Analysis of a genotype 1b strain from a single-source HCV outbreak in which HLA-B 57 was not protective revealed sequence variations that interfere with immunogenicity, thereby preventing HLA-B 57-mediated immune pressure. CONCLUSIONS: Our data indicate a role of HLA-B 57-restricted CD8(+) T-cell responses in mediating spontaneous clearance and evolution in HCV infection, and viral strains containing epitope variants that are less recognized abrogate the protective effects of HLA-B 57. The finding that HLA-B 57-mediated antiviral immunity is associated with control of both human immunodeficiency virus-1 and HCV suggests a common shared mechanism of a successful immune response against persistent viruses.},
  address                = {United States},
  article-doi            = {10.1053/j.gastro.2010.09.042},
  article-pii            = {S0016-5085(10)01422-8},
  completed              = {20110328},
  electronic-issn        = {1528-0012},
  electronic-publication = {20100924},
  grantno                = {R01 AI069939-05/AI/NIAID NIH HHS/United States},
  history                = {2011/03/29 06:00 [medline]},
  issue                  = {2},
  keywords               = {Aged, Aged, 80 and over, CD8-Positive T-Lymphocytes/immunology, Cohort Studies, Epitopes/immunology, Female, HLA-B Antigens/genetics/*immunology, Hepacivirus/genetics/*immunology, Hepatitis C, Chronic/*immunology/*virology, Humans, Male, Middle Aged, Mutation},
  language               = {eng},
  linking-issn           = {0016-5085},
  location-id            = {10.1053/j.gastro.2010.09.042 [doi]},
  manuscript-id          = {NIHMS239995},
  nlm-unique-id          = {0374630},
  owner                  = {NLM},
  publication-status     = {ppublish},
  registry-number        = {0 (HLA-B57 antigen)},
