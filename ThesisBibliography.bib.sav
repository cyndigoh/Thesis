  keywords               = {Biomarkers/*blood, Blood Proteins/analysis, Cluster Analysis, Demography, Gestational Age, Humans, Immunoassay, Infant, Newborn, Infant, Premature/*blood, Infant, Premature, Diseases/*blood/*diagnosis, Infection/*blood/*diagnosis, Multivariate Analysis},
  language               = {eng},
