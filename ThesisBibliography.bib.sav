  keywords               = {Base Composition/genetics, Bayes Theorem, Biotinylation, Exons/genetics, Genomics/*methods, Humans, Nucleic Acid Hybridization, Oligonucleotide Array Sequence Analysis, Oligonucleotides/*metabolism, Polymerase Chain Reaction, Polymorphism, Single Nucleotide/genetics, Reproducibility of Results, Sensitivity and Specificity, Sequence Analysis, DNA/*methods},
  language               = {eng},
