  keywords               = {Acute Lung Injury/*genetics/pathology, Angiogenic Proteins/*genetics, Animals, *Endothelium, Vascular/pathology, Epigenesis, Genetic/*genetics, Kidney/pathology, Liver/pathology, Lung/pathology, Mice, Microvessels/pathology, Sepsis/*genetics/pathology},
  language               = {eng},
