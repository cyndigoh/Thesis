  history                = {2010/08/11 06:00 [medline]},
  issue                  = {4},
  keywords               = {Genome, Human, Genome-Wide Association Study/*methods, Humans, Phenotype, *Polymorphism, Single Nucleotide, *Quantitative Trait Loci},
  language               = {eng},
  linking-issn           = {1553-7390},
  location-id            = {10.1371/journal.pgen.1000888 [doi]},
  nlm-unique-id          = {101239074},
  owner                  = {NLM},
  publication-status     = {epublish},
  revised                = {20181113},
  source                 = {PLoS Genet. 2010 Apr 1;6(4):e1000888. doi: 10.1371/journal.pgen.1000888.},
