@Article{Vayssier-Taussat2014,
  author                 = {Vayssier-Taussat, Muriel and Albina, Emmanuel and Citti, Christine and Cosson, Jean-Franois and Jacques, Marie-Agnes and Lebrun, Marc-Henri and Le Loir, Yves and Ogliastro, Mylene and Petit, Marie-Agnes and Roumagnac, Philippe and Candresse, Thierry},
