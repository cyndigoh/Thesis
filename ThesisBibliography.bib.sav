  history                = {2016/04/20 06:00 [medline]},
  issue                  = {12},
  keywords               = {Amino Acid Sequence, Cell Line, Chromatography, Liquid, DNA Damage/*physiology, Epstein-Barr Virus Infections/*metabolism, Gene Expression Regulation, Viral, Herpesvirus 4, Human/*metabolism, Humans, Immunoblotting, Mitosis/*physiology, Molecular Sequence Data, Phosphorylation, Protein-Serine-Threonine Kinases/*metabolism, Proteomics/methods, Signal Transduction/physiology, Tandem Mass Spectrometry, Viral Proteins/*metabolism, Virus Replication/*physiology},
  language               = {eng},
