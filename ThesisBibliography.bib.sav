  issue                  = {7},
  keywords               = {*Gene Expression Regulation, *Oligonucleotide Array Sequence Analysis, *Sequence Analysis, RNA, *Software},
  language               = {eng},
  linking-issn           = {0305-1048},
  location-id            = {10.1093/nar/gkv007 [doi]},
  nlm-unique-id          = {0411011},
  owner                  = {NLM},
  publication-status     = {ppublish},
  revised                = {20181113},
  source                 = {Nucleic Acids Res. 2015 Apr 20;43(7):e47. doi: 10.1093/nar/gkv007. Epub 2015 Jan 20.},
