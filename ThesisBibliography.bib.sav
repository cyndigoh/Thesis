@Article{Ichinohe2011,
  author                 = {Ichinohe, Takeshi and Pang, Iris K. and Kumamoto, Yosuke and Peaper, David R. and Ho, John H. and Murray, Thomas S. and Iwasaki, Akiko},
  title                  = {Microbiota regulates immune defense against respiratory tract influenza A virus infection.},
