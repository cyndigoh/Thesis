  grantno            = {P20 RR016480/RR/NCRR NIH HHS/United States},
  history            = {2016/10/19 06:00 [medline]},
  issue              = {322},
  keywords           = {Adolescent, Adult, Aged, Aged, 80 and over, Case-Control Studies, Child, Child, Preschool, Cohort Studies, Coinfection/genetics/microbiology/virology, Demography, Female, *Gene Expression Regulation, Host-Pathogen Interactions/*genetics, Humans, Male, Middle Aged, Reproducibility of Results, Respiratory Tract Infections/*diagnosis/*genetics/microbiology/virology, Signal Transduction/genetics, Young Adult},
  language           = {eng},
