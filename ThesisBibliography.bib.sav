@Article{Simner2018,
  author             = {Simner, Patricia J. and Miller, Steven and Carroll, Karen C.},
  title              = {Understanding the Promises and Hurdles of Metagenomic Next-Generation Sequencing as a Diagnostic Tool for Infectious Diseases.},
